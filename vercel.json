{
  "version": 2,
  "buildCommand": "turbo run build",
  "outputDirectory": "packages/server/dist",
  "framework": "node",
  "builds": [
    {
      "src": "packages/server/dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "packages/server/dist/index.js"
    }
  ]
}
